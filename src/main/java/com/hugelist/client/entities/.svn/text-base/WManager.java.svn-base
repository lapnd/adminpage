package com.hugelist.client.entities;

import java.util.List;

import com.hugelist.entities.Category;
import com.hugelist.entities.Manager;

public class WManager {
	
	private static final long serialVersionUID = 3L;
	private Manager account;
	private List<Category> groups;
	
	public WManager() {
		super();
	}

	public WManager(Manager account, List<Category> groups) {
		super();
		this.account = account;
		this.groups = groups;
	}

	public Manager getAccount() {
		return account;
	}

	public void setAccount(Manager account) {
		this.account = account;
	}

	public List<Category> getGroups() {
		return groups;
	}

	public void setGroups(List<Category> groups) {
		this.groups = groups;
	}
}
